// cs159lab08
#include <stdio.h>
#include <math.h>
#include <time.h>

//Function Declaration

int inputSeed (int number);
void srand (unsigned int seed);

int main (void)
{
  //Local Declarations
  
  int number;
  
  //Executable Statements
  
  int inputSeed (&number);
  srand(number);
  
  return 0;
}

void inputSeed (int* number)
{
  printf("Enter value to seed random number generator: );
  scanf("%d", &seed);
  *number = seed;
  return;
}
void srand (unsigned int seed);
{
//Local Declarations
  int range;

//Executable Statements
  range = (52 - 1) - 1;
  printf("%d", rand() % range + 1);
  printf("%d", rand() % range + 1);
  printf("%d", rand() % range + 1);
  printf("%d", rand() % range + 1);
  printf("%d", rand() % range + 1);
  return 0;
}
